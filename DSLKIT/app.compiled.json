{
  "mainScreen": "profileScreen",
  "context": {
    "dataset": []
  },
  "screens": [
    {
      "type": "screen",
      "id": "profileScreen",
      "navigationBar": {
        "title": {
          "var": "Teste"
        },
        "displayMode": "large"
      },
      "body": [
        {
          "type": "vstack",
          "alignment": "center",
          "children": [
            {
              "type": "button",
              "children": [
                {
                  "type": "text",
                  "value": "Add"
                }
              ],
              "onTap": {
                "sequence": [
                  {
                      "Array.append": {
                          "var": "dataset",
                          "value": 1
                      }
                  }
                ]
              },
              "modifiers": [
                {
                  "frame": {
                    "maxWidth": ".infinity"
                  }
                },
                {
                  "padding": 14
                },
                {
                  "cornerRadius": 20
                }
              ]
            },
            {
              "type": "list",
              "data": {"var": "dataset"},
              "visible": {"Logic.neq": [{"var": "dataset"}, []]},
              "children":
              {
                  "type": "text",
                  "value": {"var": "dataset[currentItemIndex]"},
                  "modifiers": [
                    {
                      "foreground": "#000000"
                    }
                  ]
              }
            }
          ]
        }
      ]
    }
  ]
}
